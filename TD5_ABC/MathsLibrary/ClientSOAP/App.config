<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="MathsOperations" />
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="MathsOperationsAChanged" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/MathsService/maths"
                binding="basicHttpBinding" bindingConfiguration="MathsOperations"
                contract="MathsService.IMathsOperations" name="MathsOperations" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/MathsService/mathsA"
                binding="wsHttpBinding" bindingConfiguration="MathsOperationsAChanged"
                contract="MathsService.IMathsOperations" name="MathsOperationsAChanged">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>